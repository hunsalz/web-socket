{
  "entrypoint": "demo/index.html",
  "shell": "demo/echo-demo.js",
  "fragments": [],
  "sources": [
    "src/**.js",
    "package.json",
    "demo/index.html",
    "demo/echo-demo.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "public",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false,
      "addPushManifest": false,
      "preset": "es6-bundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}